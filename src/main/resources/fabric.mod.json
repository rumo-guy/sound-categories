{
  "schemaVersion": 1,
  "id": "soundcategories",
  "version": "${version}",
  "name": "Sound Categories",
  "description": "Allows mods to define custom sound categories.",
  "authors": [
    "stashymane"
  ],
  "contact": {
    "homepage": "${modHomePage}",
    "sources": "${modSources}"
  },
  "license": "${modLicense}",
  "environment": "client",
  "mixins": [
    "soundcategories.mixins.json"
  ],
  "entrypoints": {
    "preLaunch": [
      "dev.stashy.soundcategories.SoundCategoriesPreLaunch"
    ]
  },
  "depends": {
    "fabricloader": "*",
    "minecraft": ">=1.19",
    "fabric": "*"
  },
  "custom": {
    "modmenu:clientsideOnly": true
  }
}
